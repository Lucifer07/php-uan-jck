{
    "name": "phpuan-jck/phpuan-jck",
    "type": "library",
    "description": "A Laravel Performance Profiler Library for local development with Xdebug integration",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "PhpuanJck\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpuanJck\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PhpuanJck\\Providers\\PhpuanJckServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
